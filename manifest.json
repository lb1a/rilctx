{
    "name": "pocketctx",
    "version": "0.1",
    "description": "Convenient Context Menu Entry for Pocket",
    "background_page": "background.html",
    "permissions" : [
        "tabs",
        "contextMenus",
        "notifications",
        "<all_urls>"
    ],
    "options_page": "options.html",
    "browser_action": {
        "default_icon":"link_add.png",
        "default_popup":"popup.html"
    },
    "icons" : {
        "16" : "link_add.png"
    }
}
